{
  "daemon": "nites_roomalert",
  "log_name": "nites_roomalertd",
  "query_ratelimit": 5,
  "roomalert_ip": "10.2.6.187",
  "roomalert_port": 80,
  "roomalert_query_timeout": 5,
  "roomalert_legacy_api": true,
  "sensors": [
    {
      "id": "internal_temperature",
      "label": "Internal Temp",
      "units": "\u00B0C",
      "type": "digital",
      "index": 0,
      "key": "tempc"
    },
    {
      "id": "internal_humidity",
      "label": "Internal Hum.",
      "units": "%RH",
      "type": "digital",
      "index": 0,
      "key": "humid"
    },
    {
      "id": "security_system",
      "label": "Tel. Sec. Sys",
      "values": ["TRIPPED", "SAFE"],
      "type": "switch",
      "index": 7,
      "key": "status"
    },
    {
      "id": "mains_power",
      "label": "Mains Power",
      "values": ["OFFLINE", "ONLINE"],
      "type": "switch",
      "index": 0,
      "key": "status"
    }
  ]
}